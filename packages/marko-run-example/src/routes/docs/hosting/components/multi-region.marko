style.scss {
  .multi-region {
    display: block;
    margin: 0 auto;
    max-height: fit-content;
    max-width: 100%;

    text {
      fill: $foreground-color-neutral-00;
      font-family: inherit;
      text-anchor: middle;
    }

    .server {
      fill: none;
      stroke: $foreground-color-accent;
    }

    .region {
      fill: none;
      stroke: $foreground-color-accent;
      stroke-dasharray: 1.5 7;
      stroke-width: 1.5;
    }

    .ttfb {
      fill: none;
      stroke: $foreground-color-success;
    }

    .ttfb-text {
      fill: $foreground-color-success;
    }

    .ttlb {
      fill: none;
      stroke: $foreground-color-error;
    }

    .ttlb-text {
      fill: $foreground-color-error;
    }
  }
}
